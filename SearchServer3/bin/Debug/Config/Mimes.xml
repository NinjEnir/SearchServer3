<?xml version="1.0" encoding="utf-8"?>
<WebSearch>
  <MimeTypes>
    <!-- 
	#########################################################
	Sonstige Formate erarbeiten
	#########################################################
	xslx
	docx
	xlsm
	docm
	pptx
	-->
  
    <!-- 
	#########################################################
	Sonstige Formate (Kein Player) , jedoch Thumbnails
	#########################################################
	-->
    <MimeType Postfix=".xls" 	Mimetype="application/xls"    			Player="Thumbed" 	CacheSec="3600" CreateThumb="True" />
    <MimeType Postfix=".doc" 	Mimetype="application/msword" 			Player="Thumbed" 	CacheSec="3600" CreateThumb="True" />
    <MimeType Postfix=".ppt" 	Mimetype="application/powerpoint" 		Player="Thumbed" 	CacheSec="3600" CreateThumb="True" />
    <!-- 
	#########################################################
	Sonstige Formate (Kein Player) , keine Thumbnails
	#########################################################
	-->
    <MimeType Postfix=".exe" 	Mimetype="application/octet-stream" 	Player="NoPlayer" 	CacheSec="3600" CreateThumb="False" />
    <MimeType Postfix=".dll" 	Mimetype="application/octet-stream" 	Player="NoPlayer" 	CacheSec="3600" CreateThumb="False" />
    <MimeType Postfix=".bin" 	Mimetype="application/octet-stream" 	Player="NoPlayer" 	CacheSec="3600" CreateThumb="False" />
    <MimeType Postfix=".uu" 	Mimetype="application/octet-stream" 	Player="NoPlayer" 	CacheSec="3600" CreateThumb="False" />
    <MimeType Postfix=".flv" 	Mimetype="application/x-shockwave-flash" Player="NoPlayer" 	CacheSec="3600" CreateThumb="False" />
    <!-- 
	#########################################################
	Dokumenttypen (Viewer.js)
	#########################################################
	-->
    <MimeType Postfix=".pdf" 	Mimetype="Application/pdf" 				Player="PDF" 		CacheSec="3600" CreateThumb="True" />
    <MimeType Postfix=".odf" 	Mimetype="Application/odf" 				Player="PDF" 		CacheSec="3600" CreateThumb="True" />
